[submodule ".fonts"]
	path = .fonts
	url = github.com:seletskiy/monaco-font
[submodule "<root>/usr/share/X11/xkb"]
	path = <root>/usr/share/X11/xkb
	url = github.com:reconquest/rex
[submodule ".vimperator/bundle"]
	path = .vimperator/bundle
	url = https://github.com/vimpr/vimperator-plugins
[submodule ".deadfiles"]
	path = .deadfiles
