super + Return
    urxvt -e tmux new

super + ctrl + Return
    open-at-cwd urxvt -e tmux new

super + r
    modal-run

shift + Delete
    kill-focus

super + F4
    sound-switch

super + F2
    ;volume down

super + F3
    ;volume up

super + F8
    external-monitor

super + F9
    reconfig-kbd

super + F10
    vpn-toggle

super + y
    translator

super + t
    bookmarks

super + Tab
    ;journal

super + grave
    ;kuake --terminal --bottom

super + x
    sh -c 'carcosa-ui ~/.secrets/*'

super + shift + x
    sh -c 'carcosa-ui --mode=login-password --prefix=passwords/ ~/.secrets/*'

super + Execute
    environment-info

super + F11
    sudo brightness down

super + F12
    sudo brightness up

super + backslash
    screenshot deepin

super + Delete
    upwork-suspend

super + ctrl + Delete
    jidle-toggle
