super + Return
    urxvt -e tmux new

super + control + Return
    open-at-cwd urxvt -e tmux new

super + r
    modal-run

shift + Delete
    kill-focus

super + F2
    ;volume down

super + F3
    ;volume up

super + F4
    sound-switch

super + F5
    vpn-toggle

super + F8
    systemctl --user restart monitors

super + F9
    systemctl --user restart keyboard

super + F6
    i3-frame

super + y
    translator

super + t
    bookmarks

super + control + t
    bookmarks add

super + grave
    ;kuake --terminal --bottom

super + x
    carcosa-ui

super + control + x
    carcosa-ui --mode=login-password

super + Execute
    environment-info

super + F11
    sudo brightness down

super + F12
    sudo brightness up

