#!/bin/bash

sound() {
    sed -rn 's/^##://;T;p' "$0" \
        | base64 -d \
        | zcat -9
}

stopwatch & trap "kill $!" EXIT ERR QUIT
play -q <(sound) pad 0 $(bc -l <<< 60/${1:-60}-$(soxi -D <(sound))) repeat -

