IdentityFile        ~/.ssh/id_rsa

Host *.d *.t*
    User            root
    IdentityFile    ~/.ssh/id_rsa_env

Host *.s
    User             root

Host ya*
    HostName        %h.yard.s

Host task*
    User            ngs

Host git
    User git

Host baranov
    User            guest
    HostName        192.168.4.45

Host github.com gh
    HostName        github.com
    User            git
    ControlPath     none

Host grn
   Hostname         git.rn
   User             git
   Port             7999
   ControlPath      none

Host *
   ControlMaster    auto
   ControlPersist   1h
   ControlPath      ~/.ssh/connections/%r_%h_%p
   Compression      no
   SendEnv          TERM BACKGROUND
