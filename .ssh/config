IdentityFile        ~/.ssh/id_rsa

Host *.d *.t*
    User            root
    IdentityFile    ~/.ssh/id_rsa_env

Host tx
    HostName        chef.ngs.local
    User            deployer

Host task*
    User            ngs

Host git
    user git

Host baranov
    User            guest
    HostName        192.168.4.45

Host github.com gh
    HostName        github.com
    User            git
    ControlPath     none

Host *
   ControlMaster    auto
   ControlPersist   1h
   ControlPath      ~/.ssh/connections/%r_%h_%p
   Compression      no
   SendEnv          TERM BACKGROUND
