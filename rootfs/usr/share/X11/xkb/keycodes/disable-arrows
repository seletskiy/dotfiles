    <END>  = 248;
    <HOME> = 249;
    <PGUP> = 250;
    <PGDN> = 251;
    <LEFT> = 252;
    <RGHT> = 253;
    <UP>   = 254;
    <DOWN> = 255;

    <ARLF> = 113;
    <ARRT> = 114;
    <ARUP> = 111;
    <ARDN> = 116;
};
